{
	"name": "storybook-composition",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/StorybookComposition/src",
	"projectType": "application",
	"tags": [],
	"// targets": "to see all targets run: nx show project StorybookComposition --web",
